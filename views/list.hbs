{{#if error}}
<div class="alert alert-danger" role="alert">
  {{error}}
</div>
{{/if}}
<div>
  {{#if fileUrl}}<a href="{{fileUrl}}">Download list</a> | {{/if}}
  <a href="/">Send a new file</a>
</div>
{{#unless error}}
{{#if attendees}}
<ul>
  {{#each attendees}}
  <li>User ID : {{user_id}} <br> Name : {{name}}</li>
  {{/each}}
</ul>
{{else}}
<p>There are no possible attendees on the list</p>
{{/if}}
{{/unless}}